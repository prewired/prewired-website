---
import PageLayout from "@layouts/PageLayout.astro";
import coverPhoto from "@assets/photos/sh18_standby.jpg";
---

<PageLayout
	title="404 // Page Not Found"
	desc="We cannot find the page you are looking for."
	coverImageData={coverPhoto}
	coverImageAlt="Please stand by"
>
	<p>
		If you expected to find a page here, please email <a href="mailto:hello@prewired.org">hello@prewired.org</a> with the URL of the page and what you were expecting to find. Thank you!
	</p>
</PageLayout>
